    "@react-navigation/native-stack": "^6.5.2",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/jwt-decode": "^3.1.0",
    "apollo-upload-client": "^17.0.0",
    "dayjs": "^1.11.1",
    "expo": ">=44.0.0-0 <45.0.0",
    "expo-image-picker": "~12.0.1",
    "expo-linear-gradient": "~11.0.3",
