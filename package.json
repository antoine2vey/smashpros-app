{
  "name": "smashpros-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "concurrently \"expo run:android\" \"yarn generate --watch\"",
    "ios": "concurrently \"expo run:ios\" \"yarn generate --watch\"",
    "web": "concurrently \"expo start --web\" \"yarn generate --watch\"",
    "eject": "expo eject",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-mapbox-gl/maps": "^8.6.0-beta.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/elements": "^1.3.3",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.2",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/jwt-decode": "^3.1.0",
    "apollo-upload-client": "^17.0.0",
    "dayjs": "^1.11.1",
    "expo": ">=44.0.0-0 <45.0.0",
    "expo-blur": "~11.0.0",
    "expo-calendar": "~10.1.0",
    "expo-image-picker": "~12.0.1",
    "expo-linear-gradient": "~11.0.3",
    "expo-localization": "~12.0.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "formik": "^2.2.9",
    "graphql": "^16.3.0",
    "graphql-ws": "^5.8.1",
    "i18next": "^21.6.14",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.16.2",
    "react-native": "0.64.3",
    "react-native-email-link": "^1.13.1",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-typography": "^1.4.1",
    "react-native-web": "0.17.1",
    "tailwind-rn": "^4.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.3.5"
  },
  "expo": {
    "autolinking": {
      "android": {
        "exclude": [
          "react-native-reanimated"
        ]
      }
    }
  },
  "plugins": [
    "react-native-email-link"
  ],
  "private": true
}
